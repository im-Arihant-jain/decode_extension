{
    "manifest_version": 3,
    "version": "1.1",
    "name": "Decode-Tracker",
    "action": {
        "default_popup": "index.html",
        "default_icon": "decodelogo-removebg-preview.png"
    },
    "host_permissions": [
    "http://localhost/*",
    "http://127.0.0.1/*",
    "<all_urls>"
  ],
    "permissions": [
        "tabs",
        "activeTab"
    ]
}   